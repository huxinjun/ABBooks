<!--index.wxml-->
<view class="container">








    <view class="userinfo" abc="aaa">
        <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover" animation="{{animationData}}" catchtap="test"></image>
        <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </view>
    <view class="usermotto">
        <text class="user-motto" bindtap="bindViewTap">{{motto}}</text>
    </view>

    <view wx:for="{{list}}" wx:key="*this">
        {{index}}: {{item.value}}

        <view class="listitem">
            <view class="item" style="{{item.slider}}" bindtouchstart="touchstart" bindtouchmove="touchmove" bindtouchend="touchend" bindtouchcancel="touchcancel" data-index="{{index}}"></view>
            <view class="delete" bindtap="delete" data-index="{{index}}">删除</view>
        </view>
    </view>







</view>

    <view class="bottom_bar">

        <view class="tab" bindtap="tabMainClick">
            <image src="{{tab_main_src}}"></image>
            <text style="color:{{text_color_home}}">主页</text>

        </view>
        <view class="tab" bindtap="tabMeClick">
            <image src="{{tab_me_src}}"></image>
            <text style="color:{{text_color_me}}">我的</text>
        </view>

    </view>